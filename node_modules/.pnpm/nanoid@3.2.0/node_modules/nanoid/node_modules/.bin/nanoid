#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Desktop/v-stretch/node_modules/.pnpm/nanoid@3.2.0/node_modules/nanoid/bin/node_modules:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/nanoid@3.2.0/node_modules/nanoid/node_modules:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/nanoid@3.2.0/node_modules:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/node_modules:/Users/mac/Desktop/v-stretch/node_modules:/Users/mac/Desktop/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/nanoid@3.2.0/node_modules/nanoid/bin/node_modules:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/nanoid@3.2.0/node_modules/nanoid/node_modules:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/nanoid@3.2.0/node_modules:/Users/mac/Desktop/v-stretch/node_modules/.pnpm/node_modules:/Users/mac/Desktop/v-stretch/node_modules:/Users/mac/Desktop/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../../bin/nanoid.cjs" "$@"
fi
